<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   xmlns:tools="http://schemas.android.com/tools"
   android:layout_width="match_parent"
   android:layout_height="wrap_content"
   android:orientation="vertical">

   <View
      android:id="@+id/interaction_dialog_view"
      android:layout_width="match_parent"
      android:layout_height="4dp"
      android:background="@color/interaction_text_font" />

   <android.support.v7.widget.RecyclerView
      android:id="@+id/interaction_fragment_dialog_rv_list"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:name="com.baichang.android.circle.dialog.InteractionDialogSelectFragment"
      app:layoutManager="LinearLayoutManager"
      tools:context="com.baichang.android.circle.dialog.InteractionDialogFragment"
      tools:listitem="@layout/interaction_dialog_fragment" />

</LinearLayout>
