<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
   <item>
      <selector>
         <item android:state_pressed="true">
            <shape android:shape="rectangle">
               <solid android:color="@color/interaction_layout_background" />
               <stroke
                  android:color="@color/interaction_layout_background"
                  android:width="2dp" />
            </shape>
         </item>
         <item android:state_pressed="false">
            <shape android:shape="rectangle">
               <solid android:color="@android:color/transparent" />
               <stroke
                  android:color="@color/interaction_layout_background"
                  android:width="2dp" />
            </shape>
         </item>
      </selector>
   </item>
   <item
      android:bottom="20dp"
      android:left="20dp"
      android:right="20dp"
      android:top="20dp">
      <shape android:shape="line">
         <stroke
            android:color="@color/interaction_layout_background"
            android:width="2dp" />
      </shape>
   </item>
   <item
      android:bottom="20dp"
      android:left="20dp"
      android:right="20dp"
      android:top="20dp">
      <rotate android:fromDegrees="90">
         <shape android:shape="line">
            <stroke
               android:color="@color/interaction_layout_background"
               android:width="2dp" />
         </shape>
      </rotate>
   </item>
</layer-list>