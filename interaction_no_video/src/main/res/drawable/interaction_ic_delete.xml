<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
   <item>
      <shape android:shape="oval">
         <size
            android:height="10dp"
            android:width="10dp" />
         <solid android:color="#60000000" />
      </shape>
   </item>
   <item
      android:bottom="2dp"
      android:left="2dp"
      android:right="2dp"
      android:top="2dp">
      <rotate android:fromDegrees="45">
         <shape android:shape="line">
            <stroke
               android:color="@android:color/white"
               android:width="1dp" />
         </shape>
      </rotate>
   </item>
   <item
      android:bottom="2dp"
      android:left="2dp"
      android:right="2dp"
      android:top="2dp">
      <rotate android:fromDegrees="-45">
         <shape android:shape="line">
            <stroke
               android:color="@android:color/white"
               android:width="1dp" />
         </shape>
      </rotate>
   </item>
</layer-list>