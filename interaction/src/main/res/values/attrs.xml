<?xml version="1.0" encoding="utf-8"?>
<resources>

   <!--ForceClickImageView-->
   <declare-styleable name="ForceClickImageView">
      <attr format="reference|color" name="foregroundColor" />
   </declare-styleable>
   <!--FlowLayout-->
   <declare-styleable name="FlowLayout">
      <attr name="android:orientation" />
      <attr format="enum" name="layoutDirection">
         <enum name="ltr" value="0" />
         <enum name="rtl" value="1" />
      </attr>
      <attr format="boolean" name="debugDraw" />
      <attr format="float" name="weightDefault" />
      <attr name="android:gravity" />
   </declare-styleable>
   <declare-styleable name="FlowLayout_LayoutParams">
      <attr format="boolean" name="layout_newLine" />
      <attr format="float" name="layout_weight" />
      <attr name="android:layout_gravity" />
   </declare-styleable>
   <!-- 圆环进度-->
   <declare-styleable name="RoundTimeProgressBar">
      <attr name="roundColor" format="color"/>
      <attr name="roundProgressColor" format="color"/>
      <attr name="roundWidth" format="dimension"/>
      <attr name="timeTextColor" format="color" />
      <attr name="timeTextSize" format="dimension" />
      <attr name="maxTime" format="integer"/>
      <attr name="roundStyle">
         <enum name="STROKE" value="0"/>
         <enum name="FILL" value="1"/>
      </attr>
   </declare-styleable>

</resources>